---
const { blok } = Astro.props;
const header = blok.Header;
const subHeader = blok.Subheader;
const buttonText=blok.ButtonText;
const file=blok.File;
import SectionBlock from '../Base/SectionBlock.astro'; 
---
<SectionBlock header={header} subHeader={subHeader}>
    <a href={file} download target="_blank" rel="noopener noreferrer">
        <button class="bg-gray-800 hover:bg-gray-600 text-white text-xl font-bold py-2 px-4 rounded">
            {buttonText}
        </button>
    </a>    
</SectionBlock>