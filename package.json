{
  "name": "hotel-stern",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run prebuild && npm run buildinstructions && astro dev --host",
    "start": "node ./dist/server/entry.mjs",
    "start:preview": "SSR_MODE=true node ./dist/server/entry.mjs",
    "build": "SSR_MODE=false npm run prebuild && SSR_MODE=false npm run buildinstructions && SSR_MODE=false astro build",
    "build:hybrid": "SSR_MODE=false npm run prebuild && SSR_MODE=false npm run buildinstructions && SSR_MODE=false astro build",
    "build:ssr": "SSR_MODE=true npm run prebuild && SSR_MODE=true npm run buildinstructions && SSR_MODE=true astro build",
    "preview": "astro preview --host",
    "astro": "astro",
    "prebuild": "node generateTailwindConfig.js",
    "buildinstructions": "node generateAIInstructions.js",
    "postinstall": "npm rebuild rollup",
    "font": "node scripts/changeFontCombination.js",
    "font:test": "node scripts/testAllFonts.js",
    "font:test:loop": "node scripts/testAllFonts.js --loop",
    "font:fix": "node scripts/fixFontLoading.js",
    "font:sizes": "node scripts/findHardcodedFontSizes.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.40",
    "@astrojs/check": "^0.9.3",
    "@astrojs/node": "^8.3.4",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.8.0",
    "@iconify-json/mdi": "^1.1.67",
    "@react-email/components": "^0.0.25",
    "@sentry/astro": "^8.22.0",
    "@storyblok/astro": "^4.1.0",
    "@stripe/stripe-js": "^4.9.0",
    "ai": "^3.2.39",
    "astro": "^4.15.3",
    "astro-icon": "^1.1.0",
    "astro-iconify": "^1.2.0",
    "canvas-confetti": "^1.9.3",
    "chalk": "^5.3.0",
    "cloudinary": "^2.4.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "flowbite": "^2.4.1",
    "js-yaml": "^4.1.0",
    "marked": "^13.0.3",
    "node-fetch": "^3.3.2",
    "resend": "^4.0.0",
    "rollup": "^3.20.0",
    "stripe": "^17.2.1",
    "svelte": "^4.2.18",
    "svelte-awesome": "^3.3.5",
    "swiper": "^11.1.9",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "uuid": "^11.1.0",
    "vite": "^5.3.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@astrojs/svelte": "^5.7.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/canvas-confetti": "^1.6.4",
    "@types/stripe": "^8.0.416",
    "@vitejs/plugin-basic-ssl": "^1.1.0"
  }
}
